  # Create instance: dvi2rgb_0, and set properties
  set dvi2rgb_0 [ create_bd_cell -type ip -vlnv digilentinc.com:ip:dvi2rgb:1.8 dvi2rgb_0 ]

  # Create instance: median5x5_0, and set properties
  set median5x5_0 [ create_bd_cell -type ip -vlnv user.org:user:median5x5:1.0 median5x5_0 ]
  set_property -dict [ list \
   CONFIG.H_SIZE {1650} \
 ] $median5x5_0

  # Create instance: parameter_register_0, and set properties
  set parameter_register_0 [ create_bd_cell -type ip -vlnv user.org:user:parameter_register:1.0 parameter_register_0 ]

